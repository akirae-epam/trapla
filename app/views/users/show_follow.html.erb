<% if @users.any? %>
  <div class="show-follow">
    <%= will_paginate %>
    <% @users.each do |user| %>
      <div class="follow-user">
        <div class="user-img">
          <%= render 'shared/user_image', user: user %>
        </div>
      </div>
    <% end %>
    <%= will_paginate %>
  </div>
<% end %>
